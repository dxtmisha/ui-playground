{
  "ot": {
    "value": {
      "base": {
        "size": {
          "px": {
            "_replace": {
              "pattern": "^(.*?)$",
              "replace": "$1 * 1px"
            }
          }
        }
      },
      "file|ot": "tokens/_init/ot.json#ot",
      "shade": {
        "value": {
          "0": "0",
          "20": "20",
          "40": "40",
          "60": "60",
          "80": "80",
          "100": "100",
          "200": "200",
          "300": "300",
          "400": "400",
          "500": "500",
          "600": "600",
          "700": "700",
          "800": "800",
          "900": "900"
        },
        "_category": "shade"
      },
      "light": {
        "semantic": {
          "color": {
            "value": {
              "alpha": {
                "_category": "palette-none"
              }
            },
            "_category": "palette",
            "_default": {
              "basic": "300",
              "dark": "300"
            }
          }
        },
        "asemantic": {
          "color": {
            "_category": "palette",
            "_default": {
              "basic": "300",
              "dark": "300"
            }
          }
        }
      },
      "dark": {
        "_type": "none"
      }
    },
    "_type": "var",
    "_category": "root"
  },
  "st": {
    "value": {
      "file|import-st": "tokens/_init/st.json#st",
      "file|import-light": "tokens/_init/theme/light.json#st",
      "file|import-web": "tokens/platform/web.json#st",
      "web": {
        "_type": "none"
      }
    },
    "_type": "var",
    "_category": "root"
  },
  "dark": {
    "value": {
      "ot": {
        "value": {
          "light": {
            "file|light": "tokens/_init/ot.json#ot.dark"
          }
        },
        "_type": "var"
      },
      "st": {
        "value": {
          "file|sys": "tokens/_init/theme/dark.json#st"
        },
        "_type": "var"
      }
    },
    "_category": "theme"
  },
  "font": {
    "value": {
      "basic": "{st.web.text}",
      "extras": {
        "caption": {
          "_state": true
        }
      }
    },
    "_category": "class"
  }
}
