{
  "name": "ui",
  "private": false,
  "version": "0.0.0-dev.2",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ui-component": "ts-node commands/ui.ts component",
    "build-ui-component": "ts-node commands/ui.ts component && vite build && vue-tsc --emitDeclarationOnly"
  },
  "workspaces": [
    "packages/*"
  ],
  "files": [
    "c1",
    "c2",
    "classes/*",
    "composables",
    "constructors",
    "dist",
    "functions",
    "lib/types.d.ts",
    "m2",
    "m3",
    "media/*",
    "media/flags",
    "media/templates/project",
    "packages",
    "styles",
    "types"
  ],
  "main": "./dist/ui.umd.cjs",
  "module": "./dist/ui.js",
  "exports": {
    ".": {
      "import": "./dist/ui.js",
      "require": "./dist/ui.umd.cjs",
      "types": "./lib/index.d.ts"
    },
    "./m3/*": {
      "import": "./m3/*/index.ts",
      "types": "./dist/m3/*/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "types": "./lib/index.d.ts",
  "dependencies": {
    "sass": "^1.70.0",
    "typescript": "^5.3.3",
    "vue": "^3.4.18",
    "vue-router": "^4.2.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.14",
    "@storybook/addon-interactions": "^7.6.14",
    "@storybook/addon-links": "^7.6.14",
    "@storybook/blocks": "^7.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.6.14",
    "@storybook/vue3-vite": "^7.6.14",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@vitejs/plugin-legacy": "^5.3.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "commander": "^12.0.0",
    "dotenv": "^16.4.3",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.21.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.14",
    "ts-node": "^10.9.2",
    "vite": "^5.1.1",
    "vue-tsc": "^1.8.27"
  }
}
