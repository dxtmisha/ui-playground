{
  "basic": "{d.chip}",
  "height": {
    "_default": "md"
  },
  "gradient": {
    "_rename": "palette-gradient"
  },
  "#icon": {
    "color": {
      "_rename": "palette-color"
    }
  },
  "selected": {
    "background-color": {
      "_rename": "palette-background"
    }
  },
  "outlined": {
    "_category": "style",
    "_default": true
  },
  "elevated": {
    "background-color": {
      "_rename": "palette-background"
    },
    "_category": "style"
  },
  "input": {
    "_category": "type",
    "_default": true
  },
  "assist": {
    "_category": "type"
  },
  "filter": {
    "selected": {
      "#icon": {
        "color": {
          "_rename": "color"
        }
      }
    },
    "_category": "type"
  },
  "suggestion": {
    "_category": "type"
  },
  "avatar": {
    "=?icon-width": {
      "value": "calc(var(--sys-height) - {ref.spacing.2})",
      "_type": "var"
    }
  },
  "adaptive": {
    "sm": {
      "media-max|sm": {
        "basic": {
          "value": "{??adaptive.icon}",
          "_type": "link"
        }
      }
    },
    "md": {
      "media-max|md": {
        "basic": {
          "value": "{??adaptive.icon}",
          "_type": "link"
        }
      }
    }
  },
  "@@palette": "{palette}"
}
