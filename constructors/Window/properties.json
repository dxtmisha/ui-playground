{
  "menuSizeMultiplier": "1",
  "padding": {
    "_type": "var"
  },
  "width": {
    "_var": true,
    "_style": true
  },
  "height": {
    "_var": true,
    "_style": true
  },
  "background-color": {
    "_type": "var"
  },
  "background-opacity": {
    "_type": "var"
  },
  "transform": {
    "_type": "var"
  },
  "transform-origin": {
    "_type": "var"
  },
  "#body": {
    "#context": {},
    "_settingClone": true
  },
  "#control": {},
  "~adaptive": {
    "~menu": {
      "menuSizeMultiplier": "0.25",
      "content": "'--MENU--'",
      "top": "{??sys.inset.y}",
      "right": "auto",
      "bottom": "auto",
      "left": "{??sys.inset.x}",
      "padding": {
        "value": "0",
        "_type": "property"
      },
      "background-opacity": "0",
      "transform-origin": "{??origin.x} {??origin.y}",
      "body": {
        "top": "auto",
        "max-width": "100dvw - ({??padding} * 2)",
        "max-height": "100dvh - ({??padding} * 2)"
      }
    },
    "~modal": {},
    "~modalDynamic": {
      "&.??--location": {
        "~center": {},
        "~top": {
          "align-items": "flex-start"
        },
        "~bottom": {
          "align-items": "flex-end"
        }
      }
    }
  },
  "~fullscreen": {},
  "~alignment": {
    "~center": {},
    "~top": {},
    "~right": {},
    "~bottom": {},
    "~left": {}
  },
  "~origin": {
    "~center": {},
    "~top": {},
    "~right": {},
    "~bottom": {},
    "~left": {}
  }
}
